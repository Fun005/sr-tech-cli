{
  "name": "vue-base-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "remove-git": "find . -name '.git' | xargs rm -Rf",
    "font": "font-spider src/fonts/demo.html"
  },
  "config": {
    "type": "h5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint --ext .js,.vue src"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@tp/h5utils": "^3.2.14",
    "axios-jsonp": "^1.0.4",
    "captcha-sdk": "^0.6.0",
    "core-js": "^3.6.4",
    "fastclick": "^1.0.6",
    "font-spider": "^1.3.5",
    "lodash": "^4.17.20",
    "md5": "^2.3.0",
    "uuid-js": "^0.7.5",
    "vue": "^2.6.11",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "compression-webpack-plugin": "^1.1.12",
    "eruda": "^2.3.3",
    "less": "^4.0.0",
    "less-loader": "^5.0.0",
    "postcss-px-to-viewport": "^1.1.1",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^4.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "projectInfo": {
    "desc": "乘风破浪的姐姐成团打榜",
